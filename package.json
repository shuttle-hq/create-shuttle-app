{
    "name": "create-shuttle-app",
    "version": "0.1.26",
    "bin": {
        "create-shuttle-app": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "lint": "eslint --ignore-path .gitignore --ext .js,.ts .",
        "dev": "ncc build ./index.ts -w -o ./dist/",
        "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
        "build": "npm run release",
        "copy-assets": "ncp node_modules/create-next-app/dist/index.js ./dist/create-next-app && ncp node_modules/create-next-app/dist/templates ./dist/templates"
    },
    "devDependencies": {
        "@types/node": "^18.13.0",
        "@types/prompts": "2.0.1",
        "@typescript-eslint/eslint-plugin": "^5.50.0",
        "@typescript-eslint/parser": "^5.50.0",
        "@vercel/ncc": "0.34.0",
        "chalk": "2.4.2",
        "commander": "2.20.0",
        "create-next-app": "^13.1.6",
        "eslint": "^8.33.0",
        "eslint-config-prettier": "^8.6.0",
        "ncp": "^2.0.0",
        "prompts": "2.1.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@2toad/profanity": "^2.2.0",
        "@types/adm-zip": "^0.5.0",
        "@types/command-exists": "^1.2.0",
        "@types/semver": "^7.3.13",
        "adm-zip": "^0.5.10",
        "command-exists": "^1.2.9",
        "node-fetch": "^3.3.0",
        "semver": "^7.3.8"
    },
    "engines": {
        "node": ">=14.6.0"
    }
}
