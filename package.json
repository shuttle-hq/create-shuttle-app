{
    "name": "create-shuttle-app",
    "version": "0.1.0",
    "bin": {
        "create-shuttle-app": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "dev": "ncc build ./index.ts -w -o ./dist/",
        "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
        "build": "pnpm release",
        "copy-assets:create-next-app": "ncp node_modules/create-next-app/dist/index.js ./dist/create-next-app && ncp node_modules/create-next-app/dist/templates ./dist/templates",
        "copy-assets:overrides": "ncp overrides ./dist/templates"
    },
    "devDependencies": {
        "@types/node": "^12.6.8",
        "@types/prompts": "2.0.1",
        "@vercel/ncc": "0.34.0",
        "chalk": "2.4.2",
        "commander": "2.20.0",
        "create-next-app": "^13.1.6",
        "ncp": "^2.0.0",
        "prompts": "2.1.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "@2toad/profanity": "^2.2.0",
        "@types/adm-zip": "^0.5.0",
        "@types/command-exists": "^1.2.0",
        "@types/semver": "^7.3.13",
        "adm-zip": "^0.5.10",
        "command-exists": "^1.2.9",
        "node-fetch": "^3.3.0",
        "semver": "^7.3.8"
    },
    "engines": {
        "node": ">=14.6.0"
    }
}
